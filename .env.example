# TeamWork - Virtual Dev Team Configuration
# Copy this file to .env and fill in your API keys

# Required: Anthropic API key for Claude agents
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Optional: OpenAI API key for AI-generated profile images
# If not provided, agents will use colored initials avatars
OPENAI_API_KEY=your-openai-api-key-here


# cat ~/.claude/claude.json | base64
CLAUDE_CONFIG_BASE64=<paste the long base64 string>

# Optional: Override default paths (usually not needed)
# Both Docker and local dev use these same paths at project root
DATABASE_URL=sqlite+aiosqlite:///./vteam.db
# Workspace
WORKSPACE_PATH=./workspace


# Optional: Default agent runtime (subprocess or docker)
# DEFAULT_AGENT_RUNTIME=subprocess



# Server
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Agent Runtime
DEFAULT_AGENT_RUNTIME=subprocess

# CORS
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]